let apiURL = `http://localhost:4000/api/delete-student/${id}`\r\n            let indexofArrayItem = this.Students.findIndex(i => i._id === id)\r\n            console.log(indexofArrayItem)\r\n\r\n            axios.delete(apiURL).then(()=>{\r\n                console.log('ok')\r\n                this.Students.splice(indexofArrayItem,1)\r\n            }).catch(error => {\r\n                console.log(error)\r\n            })\r\n            \r\n        }\r\n    }\r\n\r\n}\r\n</script>\r\n\r\n<style>\r\n    .btn-success {\r\n        margin-right: 10px;\r\n    }\r\n\r\n</style>"],"sourceRoot":""}]}